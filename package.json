{
  "name": "events-summary",
  "version": "1.0.1",
  "description": "Starter for GitHub Actions with TypeScript and automated releases",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build --source-map",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnandChowdhary/events-summary.git"
  },
  "keywords": [
    "github",
    "github actions",
    "nodejs",
    "typescript",
    "starter",
    "events",
    "readme",
    "readme-generator"
  ],
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnandChowdhary/events-summary/issues"
  },
  "homepage": "https://github.com/AnandChowdhary/events-summary#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.15",
    "@zeit/ncc": "^0.22.3",
    "jest": "^26.6.3",
    "semantic-release": "^17.2.2",
    "semantic-release-gitmoji": "^1.3.4",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@koj/config": "^1.2.8",
    "emoji-flags": "^1.3.0",
    "fs-extra": "^9.0.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "automock": false
  }
}
