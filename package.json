{
  "name": "notes-summary",
  "version": "1.2.0",
  "description": "Generate a summary for the notes in your GitHub repository",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build --source-map",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnandChowdhary/notes-summary.git"
  },
  "keywords": [
    "github",
    "github actions",
    "nodejs",
    "typescript",
    "notes",
    "readme",
    "readme-generator"
  ],
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnandChowdhary/notes-summary/issues"
  },
  "homepage": "https://github.com/AnandChowdhary/notes-summary#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.3",
    "@types/marked": "^4.0.7",
    "jest": "^29.3.1",
    "semantic-release": "^19.0.5",
    "semantic-release-gitmoji": "^1.4.5",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@koj/config": "^1.2.11",
    "@vercel/ncc": "^0.34.0",
    "emoji-flags": "^1.3.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.1.0",
    "markdown-to-txt": "^2.0.1",
    "prettier": "^2.7.1",
    "truncate-sentences": "^2.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "automock": false
  }
}
